{
  "go.coverOnSingleTest": true,
  "go.coverOnSingleTestFile": true,
  "go.disableConcurrentTests": true,
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [
    "--config=${workspaceFolder}/.golangci.yaml",
    "--fast",
    "-v"
  ],
  "eslint.options": {
    "overrideConfigFile": "${workspaceFolder}/ui/eslint.config.mjs"
  },
  "cSpell.words": [
    "bodyclose",
    "dupl",
    "errcheck",
    "goarch",
    "gocritic",
    "gocyclo",
    "godoc",
    "gofmt",
    "gofumpt",
    "golangci",
    "gorm",
    "gosec",
    "gosimple",
    "govet",
    "Healthz",
    "ineffassign",
    "Infof",
    "labstack",
    "ldflags",
    "logrus",
    "Nuxt",
    "prealloc",
    "sirupsen",
    "staticcheck",
    "stretchr",
    "struct",
    "swaggo",
    "testifylint",
    "todos",
    "typecheck",
    "unconvert",
    "usestdlibvars"
  ],
  "makefile.configureOnOpen": false
}
